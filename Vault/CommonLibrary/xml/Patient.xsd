                                <xsd:element name="StartDate" type="xsd:dateTime" />
                                <xsd:element name="DCDate" type="xsd:dateTime" />
                                <xsd:element name="RxNorm" type="xsd:string" />
                                <xsd:element name="NDCs">
                                  <xsd:complexType>
                                    <xsd:sequence>
                                      <xsd:element maxOccurs="unbounded" name="NDC" type="xsd:string"/>
                                    </xsd:sequence>
                                  </xsd:complexType>
                                </xsd:element>
                              </xsd:sequence>
                              <xsd:attribute name="IsOcular" type="xsd:boolean" use="required" />
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="VisualAcuities">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element maxOccurs="unbounded" name="VisualAcuity">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="Description" type="xsd:string" />
