                                      <xsd:element name="Date" type="xsd:dateTime" />
                                    </xsd:sequence>
                                  </xsd:complexType>
                                </xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="AllergyHistories">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element maxOccurs="unbounded" name="AllergyHistory">
                                  <xsd:complexType>
                                    <xsd:sequence>
                                      <xsd:element name="Description" type="xsd:string" />
                                      <xsd:element name="Date" type="xsd:dateTime" />
                                    </xsd:sequence>
                                  </xsd:complexType>
                                </xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="ROSHistories">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element maxOccurs="unbounded" name="ROSHistory" type="xsd:string"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="Medications">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element maxOccurs="unbounded" name="Medication">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="DrugName" type="xsd:string" />
                                <xsd:element name="Dose">
                                  <xsd:complexType>
                                    <xsd:simpleContent>
                                      <xsd:extension base="xsd:string">
                                        <xsd:attribute name="DoseId" type="xsd:int" use="required" />
