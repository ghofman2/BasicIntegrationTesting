                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element maxOccurs="unbounded" name="Drop">
                                  <xsd:complexType>
                                    <xsd:simpleContent>
                                      <xsd:extension base="xsd:string">
                                        <xsd:attribute name="DropId" type="xsd:int" use="required" />
                                      </xsd:extension>
                                    </xsd:simpleContent>
                                  </xsd:complexType>
                                </xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="Refractions">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element maxOccurs="unbounded" name="Refraction">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="SphereOD" type="xsd:string" />
                                <xsd:element name="CylinderOD" type="xsd:string" />
                                <xsd:element name="AxisOD" type="xsd:string" />
                                <xsd:element name="VisualActuityDistanceOD" type="xsd:string" />
                                <xsd:element name="VisualActuityNearOD" type="xsd:string" />
                                <xsd:element name="PrismOD" type="xsd:string"/>
                                <xsd:element name="AddOD" type="xsd:string" />
