                    </xsd:element>
                    <xsd:element name="IOPs">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element maxOccurs="unbounded" name="IOP">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="Description" type="xsd:string" />
                                <xsd:element name="OD" type="xsd:string" />
                                <xsd:element name="OS" type="xsd:string" />
                                <xsd:element name="DateTime" type="xsd:dateTime" />
                              </xsd:sequence>
                              <xsd:attribute name="IOPMethodID" type="xsd:int" use="required" />
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="Dilation">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="EyeLocation" type="EyeLocation"/>
                          <xsd:element name="DateTime" type="xsd:dateTime"/>
